{
  "name": "rugalib/ruga-asset-jquery-highlight",
  "description": "highlight: JavaScript text higlighting jQuery plugin",
  "homepage": "http://bartaz.github.io/sandbox.js/jquery.highlight.js",
  "type": "library",
  "require": {
    "php": "^7.4",
    "rugalib/asset-jquery": "^3.5"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.rugalib.ch/"
    }
  ],
  "authors": [
    {
      "name": "Johann Burkard",
      "homepage": "https://johannburkard.de/"
    },
    {
      "name": "Bartek Szopka",
      "homepage": "https://github.com/bartaz/sandbox.js"
    },
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\Asset\\jquery_highlight\\": "src/"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Asset\\jquery_highlight",
      "config-provider": "Ruga\\Asset\\jquery_highlight\\ConfigProvider"
    }
  }
}